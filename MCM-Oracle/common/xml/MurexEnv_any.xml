<?xml version="1.0" encoding="utf-8"?>
<Configuration>
  <Include>MurexEnv_all.xml</Include>
  <Include>MurexSystemAccounts_@ENV_NAME@.xml</Include>
  <UnixRole>@ROLE@</UnixRole>
  <EDWId>__EDWID__</EDWId>
  <mxenv>@ENV_NAME@</mxenv>
  <old_mxenv>@OLD_ENV_NAME@</old_mxenv>
  <WithMemcached>@HAS_MEMCACHED@</WithMemcached>
  <MemCachedServer>@MEMCACHED_SERVER@</MemCachedServer>
  <PatrolLogFile>@INT_APP_ROOT_PATH@/logs/SanityCheck.log</PatrolLogFile>
  <BankingProxy>
    <SOAP>
      <URL>@BANKING_PROXY_SOAP_URL@</URL>
    </SOAP>
  </BankingProxy>
  <Murex>
	<VarEngineInstances>@VARENGINE_INSTANCES@</VarEngineInstances>
    <Directory>
      <Code>@CODE_DIR_TOP@</Code>
      <Application>@APP_ROOT_PATH@</Application>
      <Root>@INT_APP_ROOT_PATH@</Root>
      <Logs>@APP_ROOT_PATH@/logs</Logs>
    </Directory>
    <AppServers>
      <Primary>
        <Server>@FILESERVER_HOST@</Server>
        <UnixUser>@ROLE@</UnixUser>
      </Primary>
    </AppServers>
    <Services>
      <FileServer>
        <Host>@FILESERVER_HOST@</Host>
        <Port>@FILESERVER_PORT@</Port>
      </FileServer>
      <XMLServer>
        <Host>@XML_SERVER_HOST@</Host>
        <Port>@XML_SERVER_PORT@</Port>
      </XMLServer>
      <Murexnet>
        <Host>@MUREXNET_HOST@</Host>
        <Port>@MUREXNET_PORT@</Port>
      </Murexnet>
	  <DocServer>
        <Host>@DOCSERVER_HOST@</Host>
        <Port>@DOCSERVER_PORT@</Port>
      </DocServer>
    </Services>
  </Murex>
  <Oracle>
    <Financial>
      <Server>@FIN_DB_SERVER_NAME@</Server>
      <Database>@FIN_DB_SERVICE_NAME@</Database>
      <Schema>@FIN_DB_SCHEMA@</Schema>
      <Port>@FIN_DB_SERVER_PORT@</Port>
      <User>@FIN_DB_USER@</User>
      <EncryptedPassword>@ORACLE_FIN_ENC_PASSWORD@</EncryptedPassword>
    </Financial>
    <Datamart>
      <Server>@DM_DB_SERVER_NAME@</Server>
      <Database>@DM_DB_SERVICE_NAME@</Database>
      <Schema>@DM_DB_SCHEMA@</Schema>
      <Port>@DM_DB_SERVER_PORT@</Port>
      <User>@DM_DB_USER@</User>
      <EncryptedPassword>@ORACLE_DM_ENC_PASSWORD@</EncryptedPassword>
    </Datamart>
  </Oracle>
  <JMS>
      <XA>@USE_XA@</XA>
    <ConnectionFactory>
      <Name>@JMS_CONNECTION_FACTORY@</Name>
      <MQServerName>@FXC_MQ_SERVER@</MQServerName>
      <MQ_Queue_Prefix>@MQ_QUEUE_PREFIX@</MQ_Queue_Prefix>
    </ConnectionFactory>
    <JNDI>
      <JavaNamingFactory>
        <URL>file://@INT_APP_ROOT_PATH@/interfaces/mq/binding</URL>
      </JavaNamingFactory>
    </JNDI>
    <MarketData>
      <Server>@FXC_MQ_SERVER@</Server>
      <Port>@FXC_MQ_PORT@</Port>
      <PublishQueue>@FXC_MQ_MDM_PUBLISH_QUEUE@</PublishQueue>
    </MarketData>
  </JMS>
  <CTRLM>
    <Agent>
      <Home>@CTRLM_AGENT_CTMHOME@</Home>
    </Agent>
    <FXCEnv>@CTRLM_FXC_ENV@</FXCEnv>
  </CTRLM>
</Configuration>
